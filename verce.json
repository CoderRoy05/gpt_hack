{
    "alias": "gpt_hack_app.vercel.app",
    "github": {
      "autoAlias": true,
      "defaultBranch": "gh-pages"  
    },
    "builds": [
      {
        "src": "your-build-directory",
        "use": "@vercel/static-build",
        "config": { "zeroConfig": true },
        "outputs": ["your-output-directory"]
      }
    ],
    "framework": "Create React App",  
    "project": "gpt_hack",
    "env": {
      "YOUR_ENV_VAR": "your-value"
    },
    "target": "serverless",
    "public": true,
    "regions": ["bru"]
  }
  